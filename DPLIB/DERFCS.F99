C SAVE INPUT ARGUMENT
      Y=X
C ABSOLUTE VALUE , ZERO VALUE
      IF(X)1,2,3
2     DERFC=1.0D0
      RETURN
1     Y=-Y
3     IF(Y.LT.17.0D0)GO TO 4
      DERFC=0.0D0
      GO TO 9
