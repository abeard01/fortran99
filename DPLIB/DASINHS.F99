      DOUBLE PRECISION FUNCTION DASINH(X)
C
C  DASINH: Double Precision ARC Sine Function
C
C  Copyright 1988 by LGMA Products
C
C  Change History:
C
C  17-July-87   V3.0   Initial Release (From Elmer Clausen)
C
      DOUBLE PRECISION X,DLOG,DSQRT
      DASINH=DLOG(X+DSQRT(X*X+1.0D0))
      RETURN
      END
