      COMPLEX*16 FUNCTION CDTANH(X) ! COMPLEX*16 HYPERBOLIC TANGENT
      COMPLEX*16 X,Y,CDEX,CDEMX,CDEXP
      CDEX=CDEXP(X)
      CDEMX=CDEXP(-X)
      Y=CDEX+CDEMX             ! DENOMINATOR
      IF(Y.EQ.(0.D0,0.0D0))THEN
         CDTANH=(1.D99,0.0D0)  ! ERROR VALUE
      ELSE
         CDTANH=(CDEX-CDEMX)/(CDEX+CDEMX)
      ENDIF
      END
